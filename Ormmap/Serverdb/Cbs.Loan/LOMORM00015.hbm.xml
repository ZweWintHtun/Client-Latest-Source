    <!--<one-to-one name="INSURAN" column="[INSUCODE]" class="LOMORM00004" insert="false" update="false" ></one-to-one>-->
    <!--<one-to-one name="INSURAN"  class="LOMORM00004" property-ref="INSUCODE" ></one-to-one>-->
    
