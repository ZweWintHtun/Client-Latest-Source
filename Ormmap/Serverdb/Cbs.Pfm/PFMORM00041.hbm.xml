  <!--//CXDAO00006.SelectMinBal-->
  <query name="CXDAO00006.SelectMinBal">
    Select new setupdto( s.CurrentMinimumBalance,s.SavingMinimumBalance ) from PFMORM00041 as s where s.CurrencyCode= : currencyCode and s.SourceBranchCode =: sourceBranch
	and s.Active = true
  </query>

	<query name="PFMDAO00041.UpdateDenostatusBySourceBr">
    update PFMORM00041 s
    set s.DenoStatus = :denoStatus,
    s.UpdatedDate = :updatedDate,
    s.UpdatedUserId = :updatedUserId
    where s.SourceBranchCode = :sourceBr and s.Active = true

  </query>

  
