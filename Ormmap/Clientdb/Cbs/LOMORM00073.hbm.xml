<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="Ace.Cbs.Loan.Dmd" namespace="Ace.Cbs.Loan.Dmd" xmlns="urn:nhibernate-mapping-2.2">

  <import class="Ace.Cbs.Loan.Dmd.LOMDTO00073, Ace.Cbs.Loan.Dmd" rename="UMDto"/>

  <class name="LOMORM00073" table="UM" >

    <id name="UMCode">
      <column name="UMCode" sql-type="varchar" not-null="true" />
    </id>

    <version name="TS" generated="always" unsaved-value="null" type="BinaryBlob">
      <column name="TS" sql-type="timestamp" not-null="true"/>
    </version>

    <property name="UMDesp">
      <column name="UMDesp" sql-type="varchar" not-null="false" />
    </property>

    <property name="USERNO">
      <column name="USERNO" sql-type="varchar" not-null="false" />
    </property>

    <property name="DATE_TIME">
      <column name="DATE_TIME" sql-type="datetime" not-null="false" />
    </property>

    <property name="Active" not-null="true"/>
    <property name="CreatedDate" not-null="true" update="false" />
    <property name="CreatedUserId" not-null="true" update="false"/>
    <property name="UpdatedDate" not-null="false" insert="false" />
    <property name="UpdatedUserId" not-null="false" insert="false"/>

  </class>

  <!--//UMDAO.CheckExist//-->
  <query name="UMDAO.CheckExist">
    select new UMDto
    (
    a.UMCode,
    a.UMDesp,
    a.USERNO,
    a.DATE_TIME)
    from LOMORM00073 as a
    where (a.UMCode = :umCode or a.UMDesp = :umDesp)
    and  a.Active = true
  </query>

  <!--//UMDAO.CheckExist2//-->
  <query name="UMDAO.CheckExist2">
    select new UMDto(
    a.UMCode,
    a.UMDesp,
    a.USERNO,
    a.DATE_TIME,
    a.Active,
    a.TS,
    a.CreatedDate,
    a.CreatedUserId,
    a.UpdatedDate,
    a.UpdatedUserId
    )
    from LOMORM00073 as a
    where (a.UMCode = :umCode or a.UMDesp = :umDesp)
  </query>


  <!--//UMDAO.SelectAll//-->
  <query name="UMDAO.SelectAll">
    select new UMDto(
    a.UMCode,
    a.UMDesp,
    a.USERNO,
    a.DATE_TIME,
    a.Active,
    a.TS,
    a.CreatedDate,
    a.CreatedUserId,
    a.UpdatedDate,
    a.UpdatedUserId
    )
    from LOMORM00073 as a
    where a.Active = true
  </query>

  <!--//UMDAO.SelectByUMCode//-->
  <query name="UMDAO.SelectByUMCode">
    select new UMDto(
    a.UMCode,
    a.UMDesp,
    a.USERNO,
    a.DATE_TIME,
    a.Active,
    a.TS,
    a.CreatedDate,
    a.CreatedUserId,
    a.UpdatedDate,
    a.UpdatedUserId
    )
    from LOMORM00073 as a
    where a.Active = true
    and a.UMCode = :umCode
  </query>

