	<!--//MNMDAO00005.SelectAll//-->
	<query name="MNMDAO00005.SelectAll">
		select new MNMDTO00005(
		t.ID,
		t.Acctno,
		t.LNo,
		t.S1,
		t.S2,
		t.S3,
		t.S4,
		t.S5,
		t.S6,
		t.S7,
		t.S8,
		t.S9,
		t.S10,
		t.S11,
		t.S12,
		t.LastInt,
		t.LastDate,
		t.Budget,
		t.UserNo,
		t.CloseDate,
		t.ACSign,
		t.TODCloseDate,
		t.TOD_SDate,
		t.SourceBr,
		t.Cur,
		t.UId,
		t.Active,
		t.TS,
		t.CreatedDate,
		t.CreatedUserId,
		t.UpdatedDate,
		t.UpdatedUserId
		)
		from MNMORM00005 as t
		where t.Active = true
		and t.SourceBr=:SourceBranchCode
	</query>
	
	<!--//MNMDAO00005.UpdateTOD_SCHARGED//-->
	<query name="MNMDAO00005.UpdateTOD_SCHARGED">
    update MNMORM00005 t set
    t.S1=:tod_month,
    t.S2=:tod_month,
    t.S3=:tod_month,
    t.S4=:tod_month,
    t.S5=:tod_month,
    t.S6=:tod_month,
    t.S7=:tod_month,
    t.S8=:tod_month,
    t.S9=:tod_month,
    t.S10=:tod_month,
    t.S11=:tod_month,
    t.S12=:tod_month,
    t.Budget=:budget,
    t.UpdatedDate=:updatedDate,
    t.UpdatedUserId=:updatedUserId
    where (t.CloseDate=null or t.CloseDate='')
    and t.SourceBr=:SourceBranchCode
    and t.Active = true
  </query>

	<!--MNMDAO00005.SelectByAccountNo-->
	<!--NLKK-->
	<query name="MNMDAO00005.SelectByAccountNo">
		select new MNMDTO00005(
		t.ID,
		t.Acctno,
		t.LNo,
		t.S1,
		t.S2,
		t.S3,
		t.S4,
		t.S5,
		t.S6,
		t.S7,
		t.S8,
		t.S9,
		t.S10,
		t.S11,
		t.S12,
		t.LastInt,
		t.LastDate,
		t.Budget,
		t.UserNo,
		t.CloseDate,
		t.ACSign,
		t.TODCloseDate,
		t.TOD_SDate,
		t.SourceBr,
		t.Cur,
		t.UId,
		t.Active,
		t.TS,
		t.CreatedDate,
		t.CreatedUserId,
		t.UpdatedDate,
		t.UpdatedUserId
		)
		from MNMORM00005 as t
		where t.Acctno =: accountNo
		and t.Active = true
		and (t.CloseDate = null or t.CloseDate = '')
	</query>

</hibernate-mapping>
