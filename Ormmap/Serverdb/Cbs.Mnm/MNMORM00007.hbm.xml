  <!--//MNMDAO00007.SelectByAccountNo//-->            <!--NLKK-->
  <query name="MNMDAO00007.SelectByCloseDate">
    select new Prevsidto(
    psi.Id,
    psi.AccountNo,
    psi.AccountSignature,
    psi.CloseDate,
    psi.Status,
    psi.Budget,
    psi.LastInt,
    psi.AccruedInt,
    psi.Month1,
    psi.Month2,
    psi.Month3,
    psi.Month4,
    psi.Month5,
    psi.Month6,
    psi.Month7,
    psi.Month8,
    psi.Month9,
    psi.Month10,
    psi.Month11,
    psi.Month12,
    psi.SourceBranchCode,
    psi.DATE_TIME,
    psi.CurrencyCode,
    psi.Active,
    psi.TS,
    psi.CreatedDate,
    psi.CreatedUserId,
    psi.UpdatedDate,
    psi.UpdatedUserId
    )
    from MNMORM00007 as psi
    where (psi.CloseDate = null or psi.CloseDate = '')
    and psi.SourceBranchCode = :sourceBr
    and psi.CurrencyCode = :cur
    and psi.Active = true

  </query>

  <query name="MNMDAO00007.SelectByBudgetYear">
    select new Prevsidto(
    psi.Id,
    psi.AccountNo,
    psi.AccountSignature,
    psi.CloseDate,
    psi.Status,
    psi.Budget,
    psi.LastInt,
    psi.AccruedInt,
    psi.Month1,
    psi.Month2,
    psi.Month3,
    psi.Month4,
    psi.Month5,
    psi.Month6,
    psi.Month7,
    psi.Month8,
    psi.Month9,
    psi.Month10,
    psi.Month11,
    psi.Month12,
    psi.SourceBranchCode,
    psi.DATE_TIME,
    psi.CurrencyCode,
    psi.Active,
    psi.TS,
    psi.CreatedDate,
    psi.CreatedUserId,
    psi.UpdatedDate,
    psi.UpdatedUserId
    )
    from MNMORM00007 as psi
    where (psi.CloseDate = null or psi.CloseDate = '')
    and psi.SourceBranchCode = :sourceBr
    and psi.CurrencyCode = :cur
    and psi.Active = true
    and psi.Budget = :Budget

  </query>


</hibernate-mapping>