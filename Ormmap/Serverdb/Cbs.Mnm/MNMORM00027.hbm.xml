<hibernate-mapping assembly="Ace.Cbs.Mnm.Dmd" namespace="Ace.Cbs.Mnm.Dmd" xmlns="urn:nhibernate-mapping-2.2">

	<import class="Ace.Cbs.Mnm.Dmd.MNMDTO00027, Ace.Cbs.Mnm.Dmd" rename="schargedto"/>
