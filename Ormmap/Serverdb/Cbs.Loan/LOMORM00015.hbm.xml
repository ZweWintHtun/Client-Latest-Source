  <!--TLMORM00018.UpdateLand_BuildingInfoByLoanNoAndSourceBr-->
  <query name="LOMDAO00015.UpdateLand_BuildingInfoByLoanNoAndSourceBr">
    update LOMORM00015 l
    set
    l.YearPB = :plBsYear,
    l.EDate = :esDate,
    l.ExpYear = :eYear,
    l.Capital = :capital,
    l.Tax = :incometax,
    l.SDEED = :sdeed,
    l.LAFFID = :landOfAffidavit,
    l.Land =:land,
    l.Char =:charaOfCustomer,
    l.GW = :gw,
    l.FSVLand = :fsvLand,
    l.FSVBLD = :fsvBuilding,
    l.Address = :address,
    l.HISLB = :historyOfLB,
    l.BPERMIT = :bConPermit,
    l.IsType = :isType,
    l.IsDate = :isDate,
    l.IsExpiredDate = :isEdate,
    l.IsAMT =:isAmt,
    l.LandLeaseSDate=:landLeaseSDate,
    l.LandLeaseEDate=:landLeaseEDate,
    l.RemarkForLand=:remarkForLand,
    l.UpdatedDate = :updatedDate,
    l.UpdatedUserId = :updatedUserId
    where l.LNo = :lno
    and l.SourceBr = :sourcebr
    and l.Active = true
  </query>

  <!--//Select Land_Building Informations By LoanNo And SourceBr//-->
  <query name="SelectLand_BuildingInfo">
    select new land_BuildingDto
    (
    l.LNo,
    l.Capital,
    l.ExpYear,
    l.EDate,
    l.Char,
    l.EVA,
    l.GW,
    l.Address,
    l.YearPB,
    l.Tax,
    l.IsAMT,
    l.IsType,
    l.IsDate,
    l.SDEED,
    l.Land,
    l.HISLB,
    l.BPERMIT,
    l.LAFFID,
    l.PADV,
    l.SType,
    l.Class,
    l.Power,
    l.DDate,
    l.FSVLand,
    l.FSVBLD,
    l.CloseDate,
    l.IsExpiredDate,
    l.SourceBr,
    l.Cur,
    l.LandLeaseSDate,
    l.LandLeaseEDate,
    l.RemarkForLand
    )
    from LOMORM00015 as l
    where l.Active = true
    and l.SourceBr =:sourcebr
    and  l.LNo = :lno
  </query>
  
</hibernate-mapping>

