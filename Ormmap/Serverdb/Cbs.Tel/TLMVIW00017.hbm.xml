	<query name="TLMDAO00059.SelectEncashRegisterOutstanding">
		select new redto
		(vw.Ebank,
		vw.ToAccountNo,
		vw.RegisterNo,
		vw.Br_Alias,
		vw.Date_Time,
		vw.Type,
		vw.ToName,
		vw.ToNRC,
		vw.Amount,
		vw.AccountSign,
		vw.OtherBank,
		vw.SourceBranchCode)
		from TLMVIW00017 as vw where vw.SourceBranchCode = :sourceBr
	</query>

</hibernate-mapping>
